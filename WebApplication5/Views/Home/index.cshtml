                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="modal-body">
                            <p>What does this look like?  Words words words words words words</p>
                            <label>Enter 2 numbers between 1 - 100:<input type="text" id="num3" name="num3"></label>
                            <input type="submit" value="Run!" onclick="fizzbuzz();" />
                            <textarea id="fizzbuzzout"></textarea>
                                <div id="fizzbuzzfunc"></div>
                        </div>
                     </div>
                    <div class="modal-body">
                        <div class="col-md-6">
                        <pre class="brush: js"> 
                        <script type="text/javascript">
         
                                function fizzBuzz() {
                                    var str = document.getElementById('num3').value;
                                    console.log(str);
                                    var numArr = str.split(",");
                                    var fizzRemain = 1;
                                    var buzzRemain = 1;
                                    var fizzBuzzRemain = 1;
                                    var fizz = numArr[0];
                                    var buzz = numArr[1];
                                    var fizzBuzz = fizz * buzz;
                                    var output = "";
                                    output += "";

                                    for (var i = 1; i <= 100; i++) {
                                        numArr[i] = parseInt(numArr[i]);
                                        fizzRemain = i % fizz;
                                        buzzRemain = i % buzz;
                                        fizzBuzzRemain = i % fizzBuzz;
                                        if (fizzBuzzRemain === 0) {

                                            output += "FizzBuzz ";
                                        }
                                        else if (fizzRemain === 0) {

                                            output += "Fizz "
                                        }
                                        else if (buzzRemain === 0) {

                                            output += "Buzz "
                                        }
                                        else {
                                            output += i + " "
                                        }

                                    }
                                    document.getElementById('fizzbuzzout').value = output;
                                }
                        </script>
                        </pre>
