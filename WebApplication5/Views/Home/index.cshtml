<section class="jscalc">
    <div class="modal fade" id="modal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h1 class="modal-title" id="myModalLabel">Integer Calculator</h1>
                </div>
                <div class="modal-body col-sm-4">
                    <label>Enter 5 Numbers<input type="text" name="num1" id="num1" /></label><br/>
                    <input type="submit" value="Maximum!" onclick="maxnumber();" />
                    <input type="submit" value="Minimum!" onclick="minnumber();" />
                    <input type="submit" value="Mean!" onclick="meannumber();" />
                    <input type="submit" value="Sum!" onclick="sumnumber();" />
                    <input type="submit" value="Product!" onclick="prdnumber();" />
                    <textarea id="jscalc-output"></textarea>
                        <!--JavaScript of 5int-->

                    <script type="text/javascript">
                        function getvaluecalc() {
                            return document.getElementById('num1').value;
                        }

                        function intoutput(output) {
                            var intoutput = document.getElementById('jscalc-output');
                            intoutput.value = output;
                        }
                            
                            
                        function maxnumber() {
                            var str = getvaluecalc();
                                console.log(str);
                                var numArr = str.split(",");
                                var highest = 0;

                                    for (var i = 0; i < numArr.length; i++) {
                                            numArr[i] = parseInt(numArr[i]);
                                        if (highest < numArr[i])
                                            highest = numArr[i];

                                    }
                                 intoutput(highest);
                          }

                        function minnumber() {
                                var str = getvaluecalc();
                                console.log(str);
                                var numArr = str.split(",");
                                var lowest = numArr[0];

                                for (var i = 0; i < numArr.length; i++) {
                                    numArr[i] = parseInt(numArr[i]);
                                    if (lowest > numArr[i])
                                        lowest = numArr[i];
                                }
                                intoutput(lowest);
                            }

                            function meannumber() {
                                var str = getvaluecalc();
                                console.log(str);
                                var numArr = str.split(",");
                                var sumnum = 0;
                                var mean = 0;

                                for (var i = 0; i < numArr.length; i++) {
                                    numArr[i] = parseInt(numArr[i]);
                                    sumnum = sumnum + numArr[i];
                                }
                                mean = sumnum / numArr.length;
                                intoutput(mean);
                            }

                            function sumNumber() {
                                var str = getvaluecalc();
                                console.log(str);
                                var numArr = str.split(",");
                                var sumnum = 0;

                                for (var i = 0; i < numArr.length; i++) {
                                    numArr[i] = parseInt(numArr[i]);
                                    sumnum = sumnum + numArr[i];
                                }
                                intoutput(sumnum);
                            }

                            function prdnumber() {
                                var str = getvaluecalc();
                                console.log(str);
                                var numArr = str.split(",");
                                var prdnum = 1;
                                for (var i = 0; i < numArr.length; i++) {
                                    numArr[i] = parseInt(numArr[i]);
                                    prdnum = prdnum * numArr[i];
                                }
                                intOutput(prdnum);
                            }
                    </script>
                </div>
                <div class="modal-footer no-margin">
                </div>
            </div>
        </div>
